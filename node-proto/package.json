{
  "name": "@stroeer/tapir-proto-v1",
  "version": "0.37.0",
  "description": "T-Online API Repo",
  "type": "commonjs",
  "engines": {
    "node": "18",
    "npm": ">=9"
  },
  "scripts": {
    "build": "tsc -b",
    "checks": "npm run lint",
    "lint": "tsc --noEmit",
    "update": "npx npm-check --update",
    "format": "prettier --write '{examples,__tests__}/**/*.ts'",
    "examples:build": "tsc -p tsconfig.examples.json",
    "examples:protobuf-es:run": "node ./dist/examples/protobuf-es/client.js"
  },
  "dependencies": {
    "@bufbuild/protobuf": "1.3.1"
  },
  "devDependencies": {
    "@tsconfig/node18": "2.0.1",
    "@tsconfig/strictest": "2.0.1",
    "@types/node-fetch": "2.6.4",
    "node-fetch": "2.6.11",
    "prettier": "2.8.8",
    "typescript": "5.0.4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/stroeer/tapir.git"
  },
  "files": [
    "stroeer"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/stroeer/tapir/issues"
  },
  "homepage": "https://github.com/stroeer/tapir#readme",
  "prettier": {
    "singleQuote": true
  }
}
